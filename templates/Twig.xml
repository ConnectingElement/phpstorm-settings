<templateSet group="Twig">
  <template name="elseif" value="{% elseif $CONDITION$ %}&#10;    $END$" description="elseif" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="set" value="{% set $VARIABLE$ = $VALUE$ %}$END$" description="set variable" toReformat="false" toShortenFQNames="true">
    <variable name="VARIABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="else" value="{% else %}&#10;    $END$" description="else" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="part" value="{% partial '$NAME$' $PARAMS$%}$END$" description="October CMS partial" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="put" value="{% put $NAME$ %}&#10;    $END$&#10;{% endput %}" description="Add put section" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
</templateSet>